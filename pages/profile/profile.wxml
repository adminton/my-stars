<view class="page">
 <view class="top">
    <button wx:if="{{!hasUserInfo && canIUse}}" open-type="getUserInfo"
    bindgetuserinfo="getUserInfo"> 获取头像昵称 </button>
    <block wx:else>
      <image class="serinfo-avatar" src="{{userInfo.avatarUrl}}"></image>
      <text class="userinfo-nickname">{{userInfo.nickName}}</text>
    </block>
  </view>
  <view class="weui-cells__title"> </view>
        <view class="weui-cells weui-cells_after-title">
            <navigator url="/pages/account/account" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="/images/account.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">账户余额</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"> </view>
            </navigator>
            <navigator url="/pages/user/user" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="/images/edit.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">个人信息</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"> </view>
            </navigator>
        </view>

  <view class="weui-cells__title"> </view>
        <view class="weui-cells weui-cells_after-title">
            <navigator url="/pages/contact/contact" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="/images/email.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">推广合作</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"> </view>
            </navigator>
            <navigator url="/pages/more/more" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="/images/setting.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">更多设置</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"> </view>
            </navigator>
        </view>
  <view class="weui-cells__title"> </view>
        <view class="weui-cells weui-cells_after-title">
            <navigator url="/pages/about/about" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
                <view class="weui-cell__hd">
                    <image src="/images/about.png" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">关于</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
            </navigator>
          </view>

  <view class="weui-cells__title"> </view>
  <view class="weui-footer">
            <view class="weui-footer__links">
                <navigator url="" class="weui-footer__link">星启科技</navigator>
                <navigator url="" class="weui-footer__link">技术部</navigator>
            </view>
            <view class="weui-footer__text">Copyright © 2019-2020 mystar.xyz</view>
        </view>
</view>